{
  "name": "node-novel-globby",
  "version": "2.0.13",
  "description": "globby with sort id/chinese/fullwidth number for node-novel",
  "keywords": [
    "alphanum",
    "alphanumeric",
    "chinese",
    "cmp",
    "compare",
    "comparison",
    "fullwidth",
    "glob",
    "globby",
    "jp",
    "natcmp",
    "natsort",
    "natural",
    "node-novel",
    "novel",
    "number",
    "order",
    "path",
    "sort",
    "string",
    "strnatcmp",
    "txt",
    "zh"
  ],
  "homepage": "https://github.com/bluelovers/ws-glob/tree/master/packages/node-novel-globby#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-glob/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-glob.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "coverage": "npx nyc yarn run test",
    "lint": "npx eslint **/*.ts",
    "test": "yarn run test:mocha",
    "test:mocha": "npx mocha --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "npm:publish": "npm publish",
    "npm:publish:lerna": "npx lerna publish --yes --cd-version patch",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json"
  },
  "dependencies": {
    "@node-novel/normalize": "^1.0.11",
    "@node-novel/sort": "^1.0.8",
    "@types/bluebird": "*",
    "@types/globby": "*",
    "bluebird": "^3.7.2",
    "glob-tree-list": "^1.0.7",
    "globby": "^11.0.1",
    "upath2": "^2.1.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "cjk-conv": "^1.2.72",
    "fs-iconv": "^3.0.5"
  }
}
